//using all functions
#include<stdio.h>
#include<conio.h>

void readarr(int a[10][10],int row,int col)
{
    int i,j;
    for(i=1;i<=row;i++)
    {
    	for(j=1;j<=col;j++)
    	{
        	printf("Enter Element %d %d : ",i,j);
        	scanf("%d",&a[i][j]);
    	}
    }
}

void mularr(int m1[10][10],int m2[10][10],int m3[10][10],int row,int col)
{
    int i,j,k;
    
    for(i=1;i<=row;i++)
    {
    	for(j=1;j<=col;j++)
    	{
    		for (k=1;k<=row;k++)
    		{
    			m3[i][j] = m3[i][j] + (m1[i][k] * m2[k][j]);
    		}
    	}
    }
}

void printarr(int m[10][10],int row,int col)
{
    int i,j;
    for(i=1;i<=row;i++)
        {
        for(j=1;j<=col;j++)
        {
            printf("%d\t ",m[i][j]);
         }
        printf("\n");
        }
}

int main()
{
    int m1[10][10],m2[10][10],m3[10][10],row,col;
    printf("Enter number of rows :");
    scanf("%d",&row);
    printf("Enter number of colomns :");
    scanf("%d",&col);
    readarr(m1,row,col);
    readarr(m2,row,col);
    mularr(m1,m2,m3,row,col);

    printarr(m3,row,col);
    return 0;
 }
