//pallindrome
#include<stdio.h>
int main()
{
	int n,m,rev,rem;
	printf("Enter the number : ");
	scanf("%d",&n);
	m=n;
	rev=0;
	while(n!=0)
	{
		rem=n%10;                   //last digit of reversed number
		rev=rev*10+rem;             //remaining digits
		n/=10;
	}
	if(rev==m)
		printf("\n%d is pallindrome\nThe Pallindrome of %d is : %d",m,m,rev);
	else
		printf("\n%d is not a pallindrome",m);
	return 0;
}
